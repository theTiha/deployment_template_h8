$ORIGIN .
$TTL 172800	; 2 days
{{ origin }}		IN SOA	{{ soa }} (
				{{ serial }} ; serial number
				43200      ; refresh (12 hours)
				900        ; retry (15 minutes)
				1814400    ; expire (3 weeks)
				7200       ; minimum (2 hours)
				)
			NS	dns01.{{ origin }}.
			NS	dns02.{{ origin }}.
$ORIGIN {{ origin }}.
dns01			A	172.16.252.2
dns02			A	172.16.252.3
